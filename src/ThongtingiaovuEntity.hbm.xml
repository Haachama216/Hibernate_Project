<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hibernate.pojo.ThongtingiaovuEntity" table="thongtingiaovu" schema="public" catalog="My_Database">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="faculty">
            <column name="faculty" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="phonenumber">
            <column name="phonenumber" sql-type="varchar(10)" length="10"/>
        </property>
    </class>
</hibernate-mapping>